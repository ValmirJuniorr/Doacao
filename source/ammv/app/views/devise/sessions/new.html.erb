<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :login %><br />
    <%= f.text_field :login %></p>
  </div>

  <div class="field">
    <%= f.label :Senha %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>
 

  <div class="actions">
  <br>  <%= f.submit "Log in" %>
  </div>
<% end %>

